RUST_LOG=info
APPFLOWY_ENVIRONMENT=production
APPFLOWY_DATABASE_URL=postgres://postgres:password@postgres:5432/postgres
APPFLOWY_REDIS_URI=redis://redis:6379
APPFLOWY_GOTRUE_JWT_SECRET={{ gotrue.jwt.secret }}
APPFLOWY_GOTRUE_JWT_EXP={{ gotrue.jwt.exp }}
APPFLOWY_GOTRUE_BASE_URL=http://gotrue:9999
APPFLOWY_GOTRUE_EXT_URL={{ scheme }}://{{ fqdn }}
APPFLOWY_GOTRUE_ADMIN_EMAIL={{ gotrue.admin.email }}
APPFLOWY_GOTRUE_ADMIN_PASSWORD={{ gotrue.admin.password }}
APPFLOWY_S3_USE_MINIO=true
APPFLOWY_S3_MINIO_URL=http://minio:9000
APPFLOWY_S3_ACCESS_KEY={{ minio.user }}
APPFLOWY_S3_SECRET_KEY={{ minio.password }}
APPFLOWY_S3_BUCKET=appflowy
